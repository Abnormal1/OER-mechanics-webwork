Context("Numeric");
#given
$m = random(1000,2000,1000);
$v = 100;
$E = random(800,990,10);

#computation
$eff = 50*$m*((100/3.6)**2)/$E;

#set tolerance
Context()->flags->set(
tolerance=>.02,
tolType=>'relative');

